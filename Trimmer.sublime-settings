{
    /**
     * Removes extra whitespace characters (including line
     * breaks) at the end of the file.
     *
     * @property    trim_eof
     * @type        {Boolean}
     * @default     {false}
     */
    "trim_eof": false,

    /**
     * Save the file after trimming.
     *
     * @property    {save_after_trim}
     * @type        {Boolean}
     * @default     {false}
     */
    "save_after_trim": false
}
